  <li>
    <strong>Protein credits:</strong>
    <%= @user.protein_credits %>
  </li>

  <li>
    <strong>Carb credits:</strong>
    <%= @user.carb_credits %>
  </li>

  <li>
    <strong>Veggie credits:</strong>
    <%= @user.veggie_credits %>
  </li>

  <li>
    <strong>Facility:</strong>
    <%= @user.facility_id %>
  </li>

</ul>

<%= link "Edit", to: user_path(@conn, :edit, @user) %>
<%= link "Back", to: user_path(@conn, :index) %>
