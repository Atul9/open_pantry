<h2>Show user</h2>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

  <li>
    <strong>Phone:</strong>
    <%= @user.phone %>
  </li>

  <li>
    <strong>Ok to text:</strong>
    <%= @user.ok_to_text %>
  </li>

  <li>
    <strong>Family members:</strong>
    <%= @user.family_members %>
