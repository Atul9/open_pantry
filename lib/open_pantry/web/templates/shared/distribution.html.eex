<tr class="js-meal-stock-row" data-stock-distribution-id="<%= @distribution.id %>">
  <td class="description">
    <%= stock_description(@distribution.stock) %>
  </td>
  <td class="packaging">
    <%= @distribution.stock.packaging %>
  </td>
  <td class="total-weight">
    <%= StockDistribution.total_weight(@distribution)%>
  </td>
  <td class="js-quantity-requested">
    <%= @distribution.quantity %>
  </td>
</tr>